{
  "name": "search-featured-results",
  "version": "1.0.0",
  "description": "A RESTful API for storing/retrieving featured search results.",
  "private": true,
  "dependencies": {
    "axios": ">=0.18.0",
    "cookie-parser": "^1.4.5",
    "cron": "^1.8.2",
    "fast-deep-equal": "^3.1.3",
    "moment": ">=2.19.3",
    "mysql2-async": "^1.0.7",
    "txstate-node-utils": "^3.0.0",
    "txstate-utils": "^1.6.1"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "chai": "^4.3.6",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.1.3",
    "should": "^13.2.3"
  },
  "scripts": {
    "start": "node ./index",
    "test": "mocha test/unit",
    "integration": "mocha --exit test/unit test/integration",
    "qual": "docker build --no-cache -t registry.its.txstate.edu/search-featured-results:qual .",
    "push": "npm run qual && docker push registry.its.txstate.edu/search-featured-results:qual",
    "deployENV": "func() { ansible-playbook -i ../Ansible/inventories/$1/inventory.ini -K ../Ansible/search-featured-results.yml $2 ; }; func",
    "checkENV": "func() { npm run deployENV -- $1 \"--check --diff\" ; }; func"
  },
  "bin": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/txstate-etc/search-featured-results"
  },
  "keywords": [
    "search",
    "curated",
    "featured",
    "google"
  ],
  "author": "nw13@txstate.edu",
  "license": "MIT"
}
